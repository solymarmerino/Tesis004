@using Tesis004.Models
@{
    ViewBag.Title = "HistoriaClinica";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">

<h2>Historia Clinica</h2>

<br />
@{
    var paciente = ViewData["paciente"] as Tesis004.Models.PacienteModel;
    var consulta = ViewData["consulta"] as Tesis004.Models.ConsultaMedicaModel;
    var fechaString = paciente.FechaNacimiento.ToString("yyyy-MM-dd");
    var tipoConsulta = (consulta.TipoConsulta != null ? (consulta.TipoConsulta.Equals("primera") ? 1 : 2 ) : 0);

    <div class="row">
        <div class="col-xl-6">
            <p>
                <label>Nombre Completo</label>
                <br />
                <input id="IptNombreCompleto" class="col-xl required" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" value="@paciente.NombreCompleto" disabled />
            </p>
        </div>
        <div class="col-md-3">
            <p>
                <label>Cedula</label>
                <br />
                <input id="IptCedula" style="text-align:left; border-radius:0.3rem; width:250px; height:40px;" value="@paciente.Cedula" disabled />
            </p>
        </div>
        <div class="col-md-3">
            <p>
                <label>Nº Historia Clinica</label>
                <br />
                <input id="IptNumeroHistoriaClinica" style="text-align:left; border-radius:0.3rem; width:200px; height:40px;" value="@paciente.NumHistoriaClinica" disabled />
        </div>
    </div>

    <div class="container">
        <br>
        <!-- Creación de barra de navegacion de la historia clinica -->
        <ul class="nav nav-tabs" role="tablist">
            <li class="nav-item">
                <a class="nav-link active" data-toggle="tab" href="#fichamedica">Ficha Médica</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#antecedentes">Antecedentes</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#atencionesprevias">Atenciones Previas</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#tablasimc">Tablas de IMC</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#curvascrecimiento">Pediatria</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#vacunas">Vacunas</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#odontograma">Odontograma</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#resultadoexamenes">Resultado de Examenes</a>
            </li>
        </ul>

        <!-- Contenido de cada sección de la historia clinica -->
        <div class="tab-content">
            <!-- Creacion de sección de ficha medica-->
            <div id="fichamedica" class="container tab-pane active">
                <br />
                <!--Creacion de seccion de revicion de datos personles-->
                <div class="card bg-info text-white">
                    <div class="card-header">
                        DATOS PERSONALES
                    </div>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-xl-3">
                            <p>
                                <label>Ocupación</label>
                                <br />
                                <input name="Ocupacion" class="required number" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" value="@paciente.Ocupacion" />
                            </p>
                        </div>
						<div class="col-xl-3">
							<p>
								<label>Etnia</label>
								<br>
								<select id="Etnia" name="Etnia" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;">
									<option value="0"> Seleccionar </option>
									@{
										var etnias = ViewData["etnias"] as List<Tesis004.Models.ParametroModel>;
										foreach (var etnia in etnias)
											{
												if (paciente.Etnia == etnia.Identificador)
												{
													<option value="@etnia.Identificador" selected> @etnia.Valor </option>
												}
												else
												{
													<option value="@etnia.Identificador"> @etnia.Valor </option>
												}
											}
										}
								</select>
							</p>
						</div>
						<div class="col-xl-3">
							<p>
								<label>Discapacidad</label>
								<br />
								<input id="CboxDiscapacidad" type="checkbox" style="text-align:center; border-radius:0.3rem; width:15px; height:15px;" @(Model != null ? (Model.Discapacidad ? "checked" : "") : "") /> Discapacidad
							</p>
						</div>
                        <div class="col-xl-3">
                            <p>
                                <label>Fecha de Nacimiento</label>
                                <br />
                                <input name="FechaNacimiento" class="required" type="date" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" value="@fechaString"/>
                            </p>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2">
                            <p>
                                <label>Edad</label>
                                <br />
                                <input name="TipoSangre" class="required" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" value="@paciente.Edad"/>
                            </p>
                        </div>
                        <div class="col-xl-3">
							<p>
								<label>Genero</label>
								<br>
								<select id="Genero" name="Genero" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;">
									<option value="0"> Seleccionar </option>
									@{
										var generos = ViewData["generos"] as List<Tesis004.Models.ParametroModel>;
										foreach (var genero in generos)
										{
											if (paciente.Sexo == genero.Identificador)
											{
												<option value="@genero.Identificador" selected> @genero.Valor </option>
											}
											else
											{
												<option value="@genero.Identificador"> @genero.Valor </option>
											}
										}
									}
								</select>
							</p>
                        </div>
                        <div class="col-md-3">
							<p>
								<label>Estado Civil</label>
								<br />
								<select id="EstadoCivil" name="EstadoCivil" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;">
									<option value="0"> Seleccionar </option>
									@{
										var estados = ViewData["estados"] as List<Tesis004.Models.ParametroModel>;
										foreach (var estado in estados)
										{
											if (paciente.EstadoCivil == estado.Identificador)
											{
												<option value="@estado.Identificador" selected> @estado.Valor </option>
											}
											else
											{
												<option value="@estado.Identificador"> @estado.Valor </option>
											}
										}
									}
								</select>
							</p>
                        </div>
                        <div class="col-md-2">
							<p>
								<label>Tipo de Sangre</label>
								<br />
								<select id="TipoSangre" name="TipoSangre" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;">
									<option value="0"> Seleccionar </option>
									@{
										var tipos = ViewData["tipos"] as List<Tesis004.Models.ParametroModel>;
										foreach (var tipo in tipos)
										{
											if (paciente.TipoSangre == tipo.Identificador)
											{
												<option value="@tipo.Identificador" selected> @tipo.Valor </option>
											}
											else
											{
												<option value="@tipo.Identificador"> @tipo.Valor </option>
											}
										}
									}
								</select>
							</p>
                        </div>
                    </div>
                </div>
                <!--Creacion de seccion de revicion de signos vitales-->
                <div class="card bg-info text-white">
                    <div class="card-header">
                        SIGNOS VITALES
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-md-3">
                        <p>
                            <label>Peso (kg)</label>
                            <input id="Peso" name="Peso" class="col-sm required number" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" disabled/>
                        </p>
                    </div>
                    <div class="col-md-3">
                        <p>
                            <label>Talla (cm)</label>
                            <input id="Talla" name="Talla" class="required number" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" disabled/>
                        </p>
                    </div>
                    <div class="col-md-3">
                        <p>
                            <label>Presion Arterial</label>
                            <input id="PA" name="PA" class="required number" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" disabled/>
                        </p>
                    </div>
                    <div class="col-md-3">
                        <p>
                            <label>Temperatura</label>
                            <input id="Temperatura" name="Temperatura" class="required number" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" disabled/>
                        </p>
                    </div>
                    <div class="col-md-3">
                        <p>
                            <label>FC</label>
                            <input id="FC" name="FC" class="required number" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" disabled/>
                        </p>
                    </div>
                    <div class="col-md-3">
                        <p>
                            <label>FR</label>
                            <input id="FR" name="FR" class="required number" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" disabled/>
                        </p>
                    </div>
                    <div class="col-md-3">
                        <p>
                            <label>IMC</label>
                            <input id="IMC" name="IMC" class="required number" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" disabled/>
                        </p>
                    </div>
                    <div class="col-md-3">
                        <p>
                            <label>Saturacion</label>
                            <input id="Saturacion" name="Saturacion" class="required number" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" disabled/>
                        </p>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xl-12">
                        <p>
                            <label>Observaciones Enfermeria</label>
                            <br />
                            <textarea id="ObservacionesEnfermeria"  class="col-xl required number" type="text" rows="2" style="text-align:left; border-radius:0.3rem; width:100%; height:100%;" disabled></textarea>
                        </p>
                    </div>
                </div>
                <div class="text-center">
                    <button style="background-color:darkturquoise; border-bottom-color:darkturquoise;color:white; border-radius:0.3rem; width:200px; height:40px; cursor: pointer" type="submit" onclick="validarFormularioSimple()">HISTORICO</button>
                    <button style="background-color:darkturquoise; border-bottom-color:darkturquoise;color:white; border-radius:0.3rem; width:200px; height:40px; cursor: pointer" type="submit" onclick="validarFormularioSimple()">PESO</button>
                    <button style="background-color:darkturquoise; border-bottom-color:darkturquoise;color:white; border-radius:0.3rem; width:200px; height:40px; cursor: pointer" type="submit" onclick="validarFormularioSimple()">TALLA</button>
                    <button style="background-color:darkturquoise; border-bottom-color:darkturquoise;color:white; border-radius:0.3rem; width:200px; height:40px; cursor: pointer" type="submit" onclick="validarFormularioSimple()">TA</button>
                </div>
                <br />
                <!--Creacion de seccion de ingreso de datos de consulta-->
                <div class="card bg-info text-white">
                    <div class="card-header">
                        DATOS DE CONSULTA
                    </div>
                </div>
                <input id="ConsultaMedicaID" value="@consulta.ConsultaMedicaID" hidden/>
                <br />
                <div class="row">
                    <div class="col-xl-3">
                        <label>Tipo de consulta:</label>
                    </div>
                    <div class="col-xl-3">
                        <input id="Primera" name="TipoConsulta" value="primera" type="radio" @(tipoConsulta == 1 ? "checked" : "")/> Primera
                    </div>
                    <div class="col-xl-3">
                        <input id="Subsecuente" name="TipoConsulta" value="subsecuente" type="radio" @(tipoConsulta == 2 ? "checked" : "")/> Subsecuente
                    </div>
                </div>
                <div class="row">
                    <div class="col-xl-3">
                        <label>Motivo de consulta</label>
                    </div>
                    <br />
                    <div class="col-xl-12">
                        <textarea id="motivoconsulta" name="motivoconsulta" class="col-xl required number" type="text" rows="3" style="text-align:left; border-radius:0.3rem; width:100%; height:100%;">@consulta.MotivoConsulta</textarea>
                    </div>
                </div>
                <br />
                <!--Creacion de seccion de ingreso de subjetivo-->
                <div class="card bg-info text-white">
                    <div class="card-header">
                        SUBJETIVOS (REVISIÓN ACTUAL DE ORGANOS Y SISTEMAS)
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xl-3">
                        <label>Descripción General</label>
                        <a name="Identificador" title="Describir lo mencionado por el paciente">
                            <i class="fa fa-info-circle" style="color:dodgerblue"></i>
                        </a>
                    </div>
                    <br />
                    <div class="col-xl-12">
                        <textarea name="descripcionsubjetivo" class="col-xl required number" type="text" rows="3" style="text-align:left; border-radius:0.3rem; width:100%; height:100%;"></textarea>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xl-3">
                        <label>Subjetivo</label>
                        <br />
                        <select id="sltSubjetivo" name="sltSubjetivo" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;">
                            <option value="0"> Seleccionar </option>
                            @{
                                var subjetivos = ViewData["subjetivos"] as List<Tesis004.Models.ParametroModel>;
                                foreach (var subjetivo in subjetivos)
                                {
                                    <option value="@subjetivo.Identificador"> @subjetivo.Valor </option>
                                }
                            }
                        </select>
                    </div>
                    <div class="col-xl-8">
                        <label>Descripción</label>
                        <br />
                        <input id="descripcionsubjetivo" name="descripcionsubjetivo" class="col-xl required number" type="text" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" />
                    </div>
                    <div class="col-xl-1">
                        <p>
                            <br />
                            <button id="btnSubjetivo" name="Identificador" title="Ingresar subjetivo especifico" style="align-content:center; background-color:darkturquoise; border-bottom-color:darkturquoise;color:white; border-radius:0.3rem; width:35px; height:35px; cursor: pointer" >
                                <i class="fas fa-plus-square"></i>
                            </button>
                        </p>
                    </div>
                    <br />
                    <div class="table-responsive">
                        <br />
                        <table id="tblSubjetivo" class="table">
                           
                        </table>
                    </div>
                </div>
                <br />
                <!--Creacion de seccion de ingreso de objetivo-->
                <div class="card bg-info text-white">
                    <div class="card-header">
                        OBJETIVO (EXAMEN FISICO REGIONAL SISTEMATICO)
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xl-3">
                        <label>Descripción General</label>
                        <a name="Identificador" title="Describir de lo evaluado por el medico">
                            <i class="fa fa-info-circle" style="color:dodgerblue"></i>
                        </a>
                    </div>
                    <br />
                    <div class="col-xl-12">
                        <textarea name="descripcionobjetivo" class="col-xl required number" type="text" rows="3" style="text-align:left; border-radius:0.3rem; width:100%; height:100%;"></textarea>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xl-3">
                        <label>Objetivo</label>
                        <br />
                        <select id="sltObjetivo" name="sltObjetivo" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;">
                            <option value="0"> Seleccionar </option>
                            @{
                                var objetivos = ViewData["objetivos"] as List<Tesis004.Models.ParametroModel>;
                                foreach (var objetivo in objetivos)
                                {
                                    <option value="@objetivo.Identificador"> @objetivo.Valor </option>
                                }
                            }
                        </select>
                    </div>
                    <div class="col-xl-8">
                        <label>Descripción</label>
                        <br />
                        <input name="descripcionobjetivo" class="col-xl required number" type="text" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" />
                    </div>
                    <div class="col-xl-1">
                        <p>
                            <br />
                            <button name="Identificador" title="Ingresar objetivo especifico" style="background-color:darkturquoise; border-bottom-color:darkturquoise;color:white; border-radius:0.3rem; width:35px; height:30px; cursor: pointer" >
                                <i class="fas fa-plus-square"></i>
                            </button>
                        </p>
                    </div>
                    <div class="table-responsive">
                        <br />
                        <table id="tblObjetivo"class="table">
                            
                        </table>
                    </div>
                </div>
                <br />
                <!--Creacion de seccion de ingreso de analisis del paciente-->
                <div class="card bg-info text-white">
                    <div class="card-header">
                        ANÁLISIS
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xl-8">
                        <label>Descripción</label>
                    </div>
                    <div class="col-xl-12">
                        <textarea id="descripcionanalisis" name="descripcionanalisis" class="col-xl required number" type="text" rows="3" style="text-align:left; border-radius:0.3rem; width:100%; height:100%;">@consulta.Analisis</textarea>
                    </div>
                </div>
                <br />
                <!--Creacion de seccion de ingreso del plan para el paciente-->
                <div class="card bg-info text-white">
                    <div class="card-header">
                        PLAN
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xl-8">
                        <label>Descripción</label>
                    </div>
                    <div class="col-xl-12">
                        <textarea id="descripcionplan" name="descripcionplan" class="col-xl required number" type="text" rows="3" style="text-align:left; border-radius:0.3rem; width:100%; height:100%;">@consulta.PlanTratamiento</textarea>
                    </div>
                </div>
                <br />
                <!--Creacion de seccion de ingreso de diagnostico-->
                <div class="card bg-info text-white">
                    <div class="card-header">
                        DIAGNOSTICO
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-xl-6">
                        <label>Diagnostico</label>
                        <br />
                        <input name="enfermedad" class="col-xl required number" type="text" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" />
                    </div>
                    <div class="col-xl-2">
                        <label>CIE-10</label>
                        <br />
                        <input name="cie10" class="col-xl required number" type="text" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;" />
                    </div>
                    <div class="col-xl-3">
                        <label>Tipo</label>
                        <br />
                        <select name="tipoenfermedad" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;">
                            <option> ----- </option>
                        </select>
                    </div>
                    <div class="col-xl-1">
                        <br />
                        <button name="Identificador" title="Ingresar diagnostico" style="background-color:darkturquoise; border-bottom-color:darkturquoise;color:white; border-radius:0.3rem; width:35px; height:30px; cursor: pointer" type="submit">
                            <i class="fas fa-plus-square"></i>
                        </button>
                    </div>
                </div>
                <div class="table-responsive">
                    <br />
                    <table class="table">
                        <tr>
                            <th scope="col">Diagnostico</th>
                            <th scope="col">CIE-10</th>
                            <th scope="col">Tipo</th>
                            <th scope="col"></th>
                        </tr>
                        <tr>
                            <td scope="col">AAAAAAA</td>
                            <td scope="col">AAA111</td>
                            <td scope="col">AAAAAAA</td>
                            <td scope="col">
                                <button name="Identificador" title="Eliminar diagnostico" style="background-color:darkturquoise; border-bottom-color:darkturquoise;color:white; border-radius:0.3rem; width:35px; height:30px; cursor: pointer" type="submit">
                                    <i class="fas fa-minus-square"></i>
                                </button>
                            </td>
                        </tr>
                    </table>
                </div>
                <!--Creacion de seccion de ingreso de receta-->
                <button data-toggle="collapse" data-target="#receta" name="Identificador" title="Ingresar medicamentos recetados" class="btn btn-info col-xl-12" style="text-align:left" border-bottom-color:darkturquoise;color:white; border-radius:0.3rem; width:100%; height:30px; cursor: pointer" type="submit">
                    RECETA 	<i class="fas fa-chevron-circle-down" style="text-align:right"></i>
                </button>
                <div id="receta" class="collapse">
                    <br />
                    <div class="row">
                        <div class="col-xl-10">
                            <label>Medicamento</label>
                        </div>
                        <div class="col-xl-1">
                            <button name="Identificador" style="background-color:darkturquoise; border-bottom-color:darkturquoise;color:white; border-radius:0.3rem; width:35px; height:30px; cursor: pointer" type="submit">
                                <i class="fas fa-print"></i>
                            </button>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xl-12">
                            <br />
                            <input name="medicamento" class="col-xl required number" type="text" style="text-align:left; border-radius:0.3rem; width:100%; height:40px;"></input>
                            <br />
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-xl-10">
                            <label>Indicaciones</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xl-12">
                            <br />
                            <input name="indicaciones" class="col-xl required number" type="text" style="text-align:left; border-radius:0.3rem; width:100%; height:40px"></input>
                            <br />
                        </div>
                    </div>
                    <br />
                    <div class="table-responsive">
                        <br />
                        <table class="table">
                            <tr>
                                <th scope="col">Medicamento</th>
                                <th scope="col">Indicación</th>
                                <th scope="col"></th>
                            </tr>
                            <tr>
                                <td scope="col">AAAAAAA</td>
